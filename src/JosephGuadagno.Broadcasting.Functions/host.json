{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "maxTelemetryItemsPerSecond" : 10,
        "excludedTypes": "Request;Exception"
      }
    },
    "logLevel": {
      "default": "Warning",
      "System": "Warning",
      "Microsoft": "Warning",
      "JosephGuadagno": "Warning",
      "Azure.Core": "Warning"
    }
  }
}